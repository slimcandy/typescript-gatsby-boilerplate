{
  "name": "type-script-gatsby-boilerplate",
  "version": "1.0.0",
  "private": true,
  "description": "Educational repository on how to set up a personal blog using Gatsby and Sanity.io",
  "author": "@slimcandy",
  "license": "MIT",
  "keywords": [
    "Gatsby",
    "Sanity.io",
    "personal blog",
    "headless CMS",
    "Portable Text",
    "code highlighting",
    "linting",
    "formatting",
    "Netlify",
    "npm"
  ],
  "scripts": {
    "develop": "gatsby develop",
    "start": "gatsby clean && gatsby develop",
    "build": "gatsby clean && gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write src/**/*.tsx",
    "lint": "eslint --fix src/**/*.tsx",
    "test": "npm run typecheck && npm run format && npm run lint"
  },
  "dependencies": {
    "@portabletext/react": "^2.0.2",
    "gatsby": "^5.7.0",
    "gatsby-plugin-image": "^3.7.0",
    "gatsby-plugin-manifest": "^5.7.0",
    "gatsby-plugin-sharp": "^5.7.0",
    "gatsby-plugin-sitemap": "^6.7.0",
    "gatsby-source-filesystem": "^5.7.0",
    "gatsby-source-sanity": "^7.6.3",
    "gatsby-transformer-sharp": "^5.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-refractor": "^2.1.7",
    "refractor": "^4.8.1"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.9",
    "@types/node": "^18.15.0",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "gatsby-plugin-postcss": "^6.7.0",
    "postcss": "^8.4.21",
    "prettier": "2.8.4",
    "tailwindcss": "^3.2.7",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": ">=18.14.2",
    "npm": ">=9.6.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/slimcandy/typescript-gatsby-boilerplate-production.git"
  },
  "bugs": {
    "url": "https://github.com/slimcandy/typescript-gatsby-boilerplate-production/issues"
  },
  "homepage": "https://gatsby.js.garden"
}
